{
    "name": "loevgaard/pakkelabels-php-sdk",
    "type": "library",
    "description": "PHP SDK for Pakkelabels.dk API",
    "keywords": [
        "php",
        "sdk",
        "pakkelabels"
    ],
    "homepage": "https://github.com/loevgaard/pakkelabels-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim Loevgaard",
            "email": "joachim@loevgaard.dk",
            "homepage": "http://www.loevgaard.dk"
        }
    ],
    "require": {
        "php": "~7.1",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/options-resolver": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.5||~6.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\Pakkelabels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Loevgaard\\Pakkelabels\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
